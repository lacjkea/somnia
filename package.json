{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "Project boilerplate with parcel and automatic deploying into the GitHub pages branch",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "dev": "parcel index.html",
    "deploy": "parcel build --experimental-scope-hoisting index.html --public-url ./ && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacjkea/parcel-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lacjkea/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/lacjkea/somnia",
  "dependencies": {
    "gh-pages": "^3.1.0",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "plyr": "^3.6.3",
    "sass": "^1.29.0",
    "snapsvg": "^0.5.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-compress": "^2.0.2"
  }
}
